// ===== Stateful Register Dispatch Actions =====
{% for f in range(1, GEN_NUM_ACTIVE_FEATURES + 1) %}
{% for r in range(1, GEN_REGS_PER_FEATURE + 1) %}

action stateful_action_sum_f{{ f }}{{ r }}() {
    ig_md.f{{ f }} = feat{{ f }}{{ r }}_sum.execute(ig_md.flow_hash);
}

action stateful_action_init_f{{ f }}{{ r }}() {
    ig_md.f{{ f }} = feat{{ f }}{{ r }}_init.execute(ig_md.flow_hash);
}

action stateful_action_min_f{{ f }}{{ r }}() {
    ig_md.f{{ f }} = feat{{ f }}{{ r }}_min.execute(ig_md.flow_hash);
}

action stateful_action_max_f{{ f }}{{ r }}() {
    ig_md.f{{ f }} = feat{{ f }}{{ r }}_max.execute(ig_md.flow_hash);
}

{% endfor %}
{% endfor %}
